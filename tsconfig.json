{
  "compilerOptions": {
  "target": "ESNext",
  "module": "ESNext",
  "moduleResolution": "Node",
  "declaration": true,
  "outDir": "./dist",
  "rootDir": "./src",
  "strict": true,
  "esModuleInterop": true,
  "skipLibCheck": true,
  "resolveJsonModule": true
},
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
